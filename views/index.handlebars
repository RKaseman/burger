
<header>
    <h1>Fish Food</h1>
</header>

<div id="wrapper">

<div>
    {{#each burgerData}}
    {{#unless this.devoured}}
    <div>
        <p>{{this.id}}.{{this.burger_name}}</p>
    </div>
    <div>
        <form>
            {{!-- send id back to db --}}
            <button type="submit" id="eat" value={{this.id}}>Eat It</button>
        </form>
    </div>
        {{/unless}}
        {{/each}}
    </div>
    <div>
        {{#each burgerData}}
        {{#if this.devoured}}
            <input type="text" placeholder="{{this.id}}.{{this.burger_name}}">
        {{/if}}
        {{/each}}
    </div>

</div>

